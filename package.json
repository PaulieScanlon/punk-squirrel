{
  "name": "tbd",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run redirects:enable && remix build",
    "dev": "npm run redirects:disable && remix dev",
    "start": "netlify serve",
    "redirects:enable": "shx cp .redirects public/_redirects",
    "redirects:disable": "shx rm -f public/_redirects",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.12.4",
    "@ffmpeg/ffmpeg": "^0.12.7",
    "@ffmpeg/util": "^0.12.1",
    "@fontsource/plus-jakarta-sans": "^5.0.17",
    "@netlify/functions": "^2.4.0",
    "@netlify/remix-adapter": "^2.2.0",
    "@octokit/rest": "^20.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@remix-run/react": "^2.3.0",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.38.5",
    "cross-env": "^7.0.3",
    "dompurify": "^3.0.6",
    "gsap": "^3.12.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.0",
    "@remix-run/serve": "^2.3.0",
    "@tailwindcss/typography": "^0.5.10",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tailwindcss": "^3.3.5"
  },
  "engines": {
    "node": ">=18"
  }
}
